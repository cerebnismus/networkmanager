      <br> <!-- EMPTY LINE!  -->

      <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-2 px-4">
        <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
          <div class="btn-group mr-2">
            <a class="btn btn-sm btn-outline-dark" href="/managenodes" role="button">Add Node</a>
          </div>
        </div>

        <div class="jumbotron jumbotron-fluid">
          <div class="container">
            <div class="table-responsive">
              <table class="table table-striped table-sm">
                <thead>
                  <tr>
                    <th scope="col">Node ID</th>
                    <th scope="col">Status</th>
                    <th scope="col">IP Addr</th>
                    <th scope="col">Caption</th>
                    <th scope="col">System Name</th>
                    <th scope="col">Comm String</th>
                    <th scope="col">Port</th>
                    <th scope="col">Version</th>
                    <th scope="col">OID</th>
                    <th scope="col">Created Date</th>
                    <th scope="col">Last Poll</th>
                  </tr>
                </thead>
                <tbody>
                  <ul>
                    <% for(let i=0; i < info.length; i++) { %>
                      <% if(info[i].completed===true) { %>
                        <% if(i % 2===0) { %>
                          <tr>
                            <td>
                              <%= info[i]._id %>
                            </td>
                            <td>
                              <%= info[i].status %>
                            </td>
                            <td>
                              <%= info[i].ipaddress %>
                            </td>
                            <td>
                              <%= info[i].nodename %>
                            </td>
                            <td>
                              <%= info[i].sysname %>
                            </td>
                            <td>
                              <%= info[i].community %>
                            </td>
                            <td>
                              <%= info[i].port %>
                            </td>
                            <td>
                              <%= info[i].vers %>
                            </td>
                            <td>
                              <%= info[i].oid %>
                            </td>
                            <td>
                              <%= info[i].created_date %>
                            </td>
                            <td>
                              <%= info[i].last_poll_date %>
                            </td>
                          </tr>
                          <% } else { %>
                          <tr>
                            <td>
                              <%= info[i]._id %>
                            </td>
                            <td>
                              <%= info[i].status %>
                            </td>
                            <td>
                              <%= info[i].ipaddress %>
                            </td>
                            <td>
                              <%= info[i].nodename %>
                            </td>
                            <td>
                              <%= info[i].sysname %>
                            </td>
                            <td>
                              <%= info[i].community %>
                            </td>
                            <td>
                              <%= info[i].port %>
                            </td>
                            <td>
                              <%= info[i].vers %>
                            </td>
                            <td>
                              <%= info[i].oid %>
                            </td>
                            <td>
                              <%= info[i].created_date %>
                            </td>
                            <td>
                              <%= info[i].last_poll_date %>
                            </td>
                          </tr>
                            <% } %>
                              <% } else { %>
                                <% if(i % 2===0) { %>
                          <tr>
                            <td>
                              <%= info[i]._id %>
                            </td>
                            <td>
                              <%= info[i].status %>
                            </td>
                            <td>
                              <%= info[i].ipaddress %>
                            </td>
                            <td>
                              <%= info[i].nodename %>
                            </td>
                            <td>
                              <%= info[i].sysname %>
                            </td>
                            <td>
