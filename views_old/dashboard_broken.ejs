          <div class="content-section">
            <div class="content-section-title">TOP 10 PACKET LOSS</div>
            <ul>
              <% for(let i=0; i < info.length; i++) { %>
                <% if(info[i].completed===true) { %>
                  <% // do nothing %>
                    <% } else { %>
                      <% if(i % 2===0) { %>
                        <li class="adobe-product">
                          <div class="products">
                            <%= info[i].nodename %> (<%= info[i].ipaddress %>)
                          </div>
                          <span class="status">
                            <% if(info[i].status==='up' ) { %>
                              <span class="status-circle green"></span>
                              <% } else if(info[i].status==='down' ) { %>
                                <span class="status-circle red"></span>
                                <% } else if(info[i].status==='unknown' ) { %>
                                  <span class="status-circle gray"></span>
                                  <% } %>
                                    Node status is <%= info[i].status %>
                          </span>
                          <div class="button-wrapper">
                            <button class="content-button status-button delete"
                              href="/nodedelete$nodeid">Delete</button>
                            <div class="menu">
                              <button class="dropdown">
                                <ul>
                                  <!-- RestApi get methods for function call  -->
